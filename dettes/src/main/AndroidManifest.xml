<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="epsi.dettes" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="epsi.dettes.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="epsi.dettes.listDettes"
            android:label="@string/title_activity_list_dettes" >
        </activity>
        <activity
            android:name="epsi.dettes.addDette"
            android:label="@string/title_activity_add_dette" >
        </activity>
    </application>

</manifest>
